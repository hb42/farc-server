{
  "name": "farc-server",
  "description": "Farc Server",
  "version": "0.0.1",
  "release": "-alpha.1",
  "copyright": "Copyright (c) 2016 Hartmut Bauer",
  "author": "hb",
  "license": "MIT",
  "private": true,
  "main": "index.html",
  "scripts": {
    "tsc": "tsc -p . --watch --outDir ../dist --rootDir .",
    "start": "live-server ",
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "inst:prod": "pushd ./dist && npm install --production && popd",
    "build:dev": "npm run clean:dist && ./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "npm run clean:dist && ./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached --env.release",
    "build:dev:spk": "npm run clean:dist && ./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached --env.spk",
    "build:prod:spk": "npm run clean:dist && ./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached --env.release --env.spk",
    "build": "npm run build:dev || ln -s ../node_modules ./dist/",
    "prod": "npm run build:prod",
    "buildspk": "npm run build:dev:spk || ln -s ../node_modules ./dist/",
    "prodspk": "npm run build:prod:spk"
  },
  "server": {},
  "dependencies": {
    "@types/body-parser": "^1.16.0",
    "@types/cookie-parser": "^1.3.29",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.0.30",
    "@types/ldapjs": "^1.0.0",
    "@types/mongoose": "^4.7.13",
    "@types/mysql": "^0.0.33",
    "@types/node": "^7.0.22",
    "@types/node-uuid": "^0.0.28",
    "@types/request": "^0.0.43",
    "@types/serve-favicon": "^2.2.27",
    "@types/serve-static": "^1.7.28",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "http": "0.0.0",
    "kerberos": "^0.0.23",
    "ldapjs": "^1.0.0",
    "lodash": "^4.13.1",
    "log4js": "^1.1.1",
    "mime": "^1.3.4",
    "mongoose": "^4.10.4",
    "mysql": "^2.11.1",
    "reflect-metadata": "^0.1.10",
    "request": "^2.74.0",
    "serve-favicon": "^2.4.3",
    "uuid": "^3.0.0"
  },
