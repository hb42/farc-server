    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.0",
    "json-loader": "^0.5.3",
    "less": "^2.5.3",
    "less-loader": "^4.0.4",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^6.0.0",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.18.0",
    "tsconfig-lint": "^0.12.0",
    "tslint": "^5.3.2",
